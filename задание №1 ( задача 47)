#include <cmath>
#include <cstdio>

/* Выкуривая одну сигарету, человек теряет пять минут жизни. Сколько лет жизни
 * он потеряет, если будет курить n дней, по m сигарет в день.
 */

int main() {
  int days;
  printf("Человек курил дней: ");
  scanf("%d", &days);
  int sigs;
  printf("Человек курил сигарет в день: ");
  scanf("%d", &sigs);
  printf("Человек потерял %d лет своей жизни\n",
         (5 * days * sigs) / (60 * 24 * 365));
}
